{
  "name": "Gas Test Car",
  "description": "Compact with 50 cid gas engine - target 5 mph/s (12s 0-60)",
  "class": "wheeled",

  "chassis": {
    "body": "compact",
    "modifier": "chassis_standard",
    "frame": "frame_standard",
    "length": 4.5,
    "width": 1.7,
    "height": 1.0
  },

  "power_plant": "gas_200cid",

  "wheels": [
    { "id": "FL", "position": [1.2, -0.5, 0.75], "tire": "front_tires" },
    { "id": "FR", "position": [1.2, -0.5, -0.75], "tire": "front_tires" },
    { "id": "RL", "position": [-1.2, -0.5, 0.75], "tire": "rear_tires" },
    { "id": "RR", "position": [-1.2, -0.5, -0.75], "tire": "rear_tires" }
  ],

  "tires": [
    {
      "id": "front_tires",
      "type": "tire_standard",
      "modifiers": ["mod_radial"],
      "size": { "radius": 0.35, "width": 0.2 }
    },
    {
      "id": "rear_tires",
      "type": "tire_standard",
      "modifiers": ["mod_radial"],
      "size": { "radius": 0.35, "width": 0.2 }
    }
  ],

  "axles": [
    {
      "id": "front",
      "wheels": ["FL", "FR"],
      "suspension": "suspension_improved",
      "brakes": "brakes_standard",
      "steering": true,
      "max_steer_angle": 0.5
    },
    {
      "id": "rear",
      "wheels": ["RL", "RR"],
      "suspension": "suspension_improved",
      "brakes": "brakes_standard",
      "steering": false
    }
  ]
}
