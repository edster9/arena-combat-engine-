{
  "name": "15 mph/s Test Car",
  "description": "Subcompact with Sport Electric - target 15 mph/s (4s 0-60)",
  "class": "wheeled",

  "chassis": {
    "body": "subcompact",
    "modifier": "chassis_standard",
    "frame": "frame_standard"
  },

  "power_plant": "electric_sport",

  "wheels": [
    { "id": "FL", "position": [1.2, -0.9, 1.1], "tire": "front_tires" },
    { "id": "FR", "position": [1.2, -0.9, -1.1], "tire": "front_tires" },
    { "id": "RL", "position": [-1.1, -0.9, 1.1], "tire": "rear_tires" },
    { "id": "RR", "position": [-1.1, -0.9, -1.1], "tire": "rear_tires" }
  ],

  "tires": [
    {
      "id": "front_tires",
      "type": "tire_standard",
      "modifiers": ["mod_radial"],
      "size": { "radius": 0.32, "width": 0.18 }
    },
    {
      "id": "rear_tires",
      "type": "tire_standard",
      "modifiers": ["mod_radial"],
      "size": { "radius": 0.32, "width": 0.18 }
    }
  ],

  "axles": [
    {
      "id": "front",
      "wheels": ["FL", "FR"],
      "suspension": "suspension_improved",
      "brakes": "brakes_standard",
      "steering": true,
      "max_steer_angle": 0.5
    },
    {
      "id": "rear",
      "wheels": ["RL", "RR"],
      "suspension": "suspension_improved",
      "brakes": "brakes_standard",
      "steering": false
    }
  ]
}